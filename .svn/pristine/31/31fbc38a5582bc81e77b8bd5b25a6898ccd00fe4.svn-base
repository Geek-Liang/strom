package com.bcdbook.wechat.pojo;

public class Message {
	private int id;//消息id
	private String keyword;//消息关键字(用来匹配用户发送过来的消息)
	private String msgType;//消息类型
	private String mediaId;//素材id(用于处理非文字消息)
	private String content;//文本消息的内容
	private String addTiem;//消息添加时间
	private String updateTime;//消息更新时间
	private String remark;//消息备注信息
	
	public Message() {
		super();
	}
	public Message(int id, String keyword, String msgType, String mediaId,
			String content) {
		super();
		this.id = id;
		this.keyword = keyword;
		this.msgType = msgType;
		this.mediaId = mediaId;
		this.content = content;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getKeyword() {
		return keyword;
	}
	public void setKeyword(String keyword) {
		this.keyword = keyword;
	}
	public String getMsgType() {
		return msgType;
	}
	public void setMsgType(String msgType) {
		this.msgType = msgType;
	}
	public String getMediaId() {
		return mediaId;
	}
	public void setMediaId(String mediaId) {
		this.mediaId = mediaId;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
}
